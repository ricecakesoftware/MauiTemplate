<?xml version="1.0" encoding="utf-8" ?>
<Shell xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
       xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
       xmlns:views="clr-namespace:RiceCakeSoftware.MauiTemplate.Views"
       xmlns:viewmodels="clr-namespace:RiceCakeSoftware.MauiTemplate.ViewModels"
       x:Class="RiceCakeSoftware.MauiTemplate.Views.MainShell"
       x:DataType="viewmodels:MainShellViewModel">
    <TabBar>
        <Tab Title="ダッシュボード" Icon="dashboard_black_48dp.svg">
            <ShellContent ContentTemplate="{DataTemplate views:DashboardPage}" />
        </Tab>
        <Tab Title="一覧" Icon="list_black_48dp.svg">
            <ShellContent ContentTemplate="{DataTemplate views:ListPage}" />
        </Tab>
    </TabBar>
</Shell>
